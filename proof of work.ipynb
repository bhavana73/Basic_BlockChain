    "    print(y)\n",
    "    while x[0:10]!=y:\n",
    "        hash_code = \"{}{}\".format(nonce ,x)\n",
    "        x= hashlib.sha256(hash_code.encode('utf-8')).hexdigest()\n",
    "        nonce=nonce+10\n",
    "    return x ,nonce-10 \n",
    "\n"
